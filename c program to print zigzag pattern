#include <stdio.h>
int main(){
    int a[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int sum;
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            if(i+j==3-1 || i==0  || i==3-1)
            {
               printf("%d\n",a[i][j]);
               sum=sum+a[i][j];
            }
            
        }
    }
    printf("%d\n",sum);
}

\\output
1
2
3
5
7
8
9
35
