#include <stdio.h>
int graetNum();

int main()
{
  int a=graetNum();
  printf("%d",a);
}
int graetNum()
{
    int x,y,graetNum;
    printf("enter the two values:\n");
    scanf("%d%d",&x,&y);
    if(x>y)
    graetNum=x;
    else
    graetNum=y;
    
    return graetNum;
}
    

\\output
5
6
6

\\to find positive 
#include <stdio.h>
int positiveNum();

int main()
{
  int a=positiveNum();
  printf("%d",a);
}
int positiveNum()

{
    int x,y,positiveNum;
    printf("enter the two values:\n");
    scanf("%d%d",&x,&y);
    if(y>0)
    positiveNum=y;
    else 
    positiveNum=x;
    
    return positiveNum;
}
    



