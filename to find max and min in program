 #include <stdio.h>
int main(){
    int arr[5]={1,2,3,4,5};
    int max;
    max=arr[0];
    int min;
    min=arr[0];
    for(int i=1; i<5; i++)
    {
        if(max<arr[i])
        {
        max=arr[i];
        
        }
        else if(min>arr[i])
        {
            min=arr[i];
        }
    }
        printf("%d\n",max);
        printf("%d\n",min);
        
}
