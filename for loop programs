1)#include <stdio.h>
int main (){
    for(int i=1; i<=5;i++)
    {
        printf("%d",i);
    }
}

\\output
12345

2)  #include <stdio.h>
  int main (){
    for(int i=5; i>=1;i--)
    {
        printf("%d",i);
    }
}

\\output
54321

3)#include <stdio.h>
int main (){
    int n=5;
    for(int i=0; i<n ; i++)
    {
        if(i%2==0)
        {
            printf("X");
        }
        else
        {
            printf("0");
        }
    }
    }
    \\output
    XOXOX
    
    4)#include <stdio.h>
int main (){
    int n=5;
    int a=1;
    for(int i=0; i<n ; i++)
    {
        if(i%2==0)
        {
        printf("%d",a);
        a++;
        }
        else
        {
            printf("*");
        }
    }
    }
    \\output
    1*2*3
    
    5) #include <stdio.h>
int main (){
    int n=5;
    int a=3;
    for(int i=0; i<n ; i++)
    {
        if(i%2==0)
        {
        printf("%d",a);
        a--;
        }
        else
        {
            printf("*");
        }
    }
    }
    \\output
    3*2*1
    
    
    6)
    #include <stdio.h>
int main (){
    int n=5;
    char ch='a';
    for(int i=0; i<n ; i++)
    {
        if(i%2==0)
        {
        printf("%c",ch);
        ch++;
        }
        else
        {
            printf("*");
        }
    }
    }
    \\output
     a*b*c
     
     7)
     #include <stdio.h>
int main (){
    int n=5;
    char ch='c';
    for(int i=0; i<n ; i++)
    {
        if(i%2==0) 
        {
        printf("%c",ch);
        ch--;
        }
        else
        {
            printf("*");
        }
    }
    }
